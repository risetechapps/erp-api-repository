{
    "name": "risetechapps/repository",
    "description": "",
    "keywords": [
        "risetechapps",
        "Rise Tech",
        "repository",
        "Model"
    ],
    "homepage": "https://github.com/risetechapps/repository",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rise Tech",
            "email": "apps@risetech.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "predis/predis": "^v2.2.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RiseTech\\Repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiseTech\\Repository\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiseTech\\Repository\\RepositoryServiceProvider"
            ],
            "aliases": {
                "Repository": "RiseTech\\Repository\\RepositoryFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0"
}
